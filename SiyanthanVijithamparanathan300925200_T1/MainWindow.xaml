<Window x:Class="SiyanthanVijithamparanathan300925200_T1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SiyanthanVijithamparanathan300925200_T1"
        mc:Ignorable="d"
        Title="Programming 3 - Test 1" Height="407.308" Width="283.077">
    <Border Padding="10">
        <StackPanel>
            <TextBlock>License Plate</TextBlock>
            <TextBox Name="txtLicensePlate" Text="{Binding LicensePlate}"></TextBox>
            <TextBlock>Mileage</TextBlock>
            <TextBox Name="txtMileage" Text="{Binding Mileage}"></TextBox>
            <TextBlock>Car Price</TextBlock>
            <TextBox Name="txtPrice" Text="{Binding Price}"></TextBox>
            <Button Margin="0 10" Click="Button_Click">Add</Button>
        </StackPanel>
    </Border>
</Window>
